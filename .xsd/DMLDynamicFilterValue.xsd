  </xsd:element>
  <xsd:complexType name="DMLDynamicFilterValue">
    <xsd:all>
      <xsd:element name="description" minOccurs="0" maxOccurs="1" type="xsd:string" />
      <xsd:element name="supportedDataTypes">
        <xsd:complexType>
          <xsd:choice minOccurs="1" maxOccurs="unbounded">
            <xsd:element name="model" type="bb:DMLModelDynamicFilterValueDataType" />
            <xsd:element name="primitive" type="bb:DMLPrimitiveDynamicFilterValueDataType" />
            <xsd:element name="key" type="bb:DMLPropertyKeyDynamicFilterValueDataType" />
          </xsd:choice>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="parametersCollection" type="bb:DMLParameterCollection" minOccurs="0" maxOccurs="1" />
      <xsd:element name="formatting" minOccurs="0" maxOccurs="1" type="xsd:string" />
    </xsd:all>
    <xsd:attribute name="name" use="required" type="xsd:string" />
    <xsd:attribute name="displayName" use="optional" type="xsd:string" />
    <xsd:attribute name="filterResolverType" use="required" type="xsd:string" />
    <xsd:attribute name="multiplicity" type="bb:DMLDynamicFilterValueType" use="optional" />
    <xsd:attribute name="isSystem" use="optional" type="xsd:boolean" />
  </xsd:complexType>
  <xsd:simpleType name="DMLDynamicFilterValueType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Single" />
      <xsd:enumeration value="Range" />
      <xsd:enumeration value="Multi" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="DMLModelDynamicFilterValueDataType">
    <xsd:attribute name="modelName" use="required" type="xsd:string" />
  </xsd:complexType>
