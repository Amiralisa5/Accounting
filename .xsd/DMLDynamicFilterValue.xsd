  <xsd:complexType name="DMLByteFieldDataType" />
  <xsd:complexType name="DMLShortFieldDataType" />
  <xsd:complexType name="DMLIntFieldDataType" />
  <xsd:complexType name="DMLLongFieldDataType" />
  <xsd:complexType name="DMLFloatFieldDataType" />
  <xsd:complexType name="DMLDecimalFieldDataType" />
  <xsd:complexType name="DMLDoubleFieldDataType" />
  <xsd:complexType name="DMLDurationFieldDataType" />
  <xsd:complexType name="DMLEnumFieldDataType">
    <xsd:attribute name="name" use="required" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="DMLModelFieldDataType">
    <xsd:attribute name="sourceModel" use="required" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="DMLCrossReferenceFieldDataType">
    <xsd:all>
      <xsd:element name="entityModels" minOccurs="0" maxOccurs="1">
        <xsd:complexType>
          <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="entityModel" type="xsd:string" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:all>
    <xsd:attribute name="interface" use="optional" type="xsd:string" />
    <xsd:attribute name="useCrossReferenceTypeWhenInterfaceProvided" use="optional" type="xsd:boolean" />
  </xsd:complexType>
  <xsd:complexType name="DMLListFieldDataType">
    <xsd:all>
      <xsd:element name="elementType" type="bb:DMLListFilteredFieldDataType" />
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="DMLListFilteredFieldDataType">
    <xsd:choice>
      <xsd:element name="object" type="bb:DMLObjectFieldDataType" />
      <xsd:element name="dictionary" type="bb:DMLDictionaryFieldDataType" />
      <xsd:element name="model" type="bb:DMLModelFieldDataType" />
      <xsd:element name="crossReference" type="bb:DMLCrossReferenceFieldDataType" />
      <xsd:element name="guid" type="bb:DMLGuidFieldDataType" />
      <xsd:element name="string" type="bb:DMLStringFieldDataType" />
      <xsd:element name="dateTime" type="bb:DMLDateTimeFieldDataType" />
      <xsd:element name="boolean" type="bb:DMLBooleanFieldDataType" />
      <xsd:element name="byte" type="bb:DMLByteFieldDataType" />
      <xsd:element name="short" type="bb:DMLShortFieldDataType" />
      <xsd:element name="int" type="bb:DMLIntFieldDataType" />
      <xsd:element name="long" type="bb:DMLLongFieldDataType" />
      <xsd:element name="float" type="bb:DMLFloatFieldDataType" />
      <xsd:element name="decimal" type="bb:DMLDecimalFieldDataType" />
      <xsd:element name="double" type="bb:DMLDoubleFieldDataType" />
      <xsd:element name="binary" type="bb:DMLBinaryFieldDataType" />
      <xsd:element name="file" type="bb:DMLFileFieldDataType" />
      <xsd:element name="image" type="bb:DMLImageFieldDataType" />
      <xsd:element name="version" type="bb:DMLVersionFieldDataType" />
      <xsd:element name="duration" type="bb:DMLDurationFieldDataType" />
      <xsd:element name="enum" type="bb:DMLEnumFieldDataType" />
      <xsd:element name="json" type="bb:DMLJsonFieldDataType" />
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="DMLBinaryFieldDataType" />
  <xsd:complexType name="DMLFileFieldDataType" />
  <xsd:complexType name="DMLImageFieldDataType" />
  <xsd:complexType name="DMLVersionFieldDataType" />
  <xsd:complexType name="DMLJsonFieldDataType">
    <xsd:attribute name="schema" use="optional" type="xsd:string" />
    <xsd:attribute name="templateSelectionPolicy" type="bb:DMLJsonTemplateSelectionPolicy" use="optional" />
  </xsd:complexType>
  <xsd:simpleType name="DMLJsonTemplateSelectionPolicy">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Property" />
      <xsd:enumeration value="BeforeCreate" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="DMLPropertyKeyDynamicFilterValueDataType">
    <xsd:attribute name="key" use="required" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="DMLParameterCollection">
    <xsd:all>
      <xsd:element name="parameters">
        <xsd:complexType>
          <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="parameter" type="bb:DMLParameter" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="defaultExpressions" minOccurs="0" maxOccurs="1">
        <xsd:complexType>
          <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="defaultExpression" type="bb:DMLDefaultExpression" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="changeExpressions" minOccurs="0" maxOccurs="1">
        <xsd:complexType>
          <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="changeExpression" type="bb:DMLChangeExpression" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="validations" minOccurs="0" maxOccurs="1">
        <xsd:complexType>
          <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="custom" type="bb:DMLCustomValidator" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="layoutExpressions" minOccurs="0" maxOccurs="1">
        <xsd:complexType>
          <xsd:choice minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="readOnlyField" type="bb:DMLFieldReadOnlyLayoutExpression" />
            <xsd:element name="disabledField" type="bb:DMLFieldDisabledLayoutExpression" />
            <xsd:element name="invisibleField" type="bb:DMLFieldInvisibleLayoutExpression" />
            <xsd:element name="readOnly" type="bb:DMLReadOnlyLayoutExpression" />
            <xsd:element name="disabled" type="bb:DMLDisabledLayoutExpression" />
            <xsd:element name="focusField" type="bb:DMLFieldFocusLayoutExpression" />
            <xsd:element name="backgroundColorField" type="bb:DMLFieldBackgroundColorLayoutExpression" />
            <xsd:element name="backgroundColor" type="bb:DMLBackgroundColorLayoutExpression" />
            <xsd:element name="foreColorField" type="bb:DMLFieldForeColorLayoutExpression" />
            <xsd:element name="foreColor" type="bb:DMLForeColorLayoutExpression" />
            <xsd:element name="captionColorField" type="bb:DMLFieldCaptionColorLayoutExpression" />
            <xsd:element name="textDecorationField" type="bb:DMLFieldTextDecorationLayoutExpression" />
            <xsd:element name="textDecoration" type="bb:DMLTextDecorationLayoutExpression" />
            <xsd:element name="fontStyleField" type="bb:DMLFieldFontStyleLayoutExpression" />
            <xsd:element name="fontStyle" type="bb:DMLFontStyleLayoutExpression" />
            <xsd:element name="filterField" type="bb:DMLFieldFilterLayoutExpression" />
            <xsd:element name="selectJsonTemplateField" type="bb:DMLFieldSelectJsonSchemaTemplateLayoutExpression" />
            <xsd:element name="filterEnumField" type="bb:DMLFieldEnumFilterLayoutExpression" />
            <xsd:element name="crossReferenceFilterField" type="bb:DMLFieldFilterCrossReferenceLayoutExpression" />
            <xsd:element name="customField" type="bb:DMLFieldCustomLayoutExpression" />
          </xsd:choice>
        </xsd:complexType>
      </xsd:element>
    </xsd:all>
    <xsd:attribute name="form" use="optional" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="DMLParameter">
    <xsd:all>
      <xsd:element name="description" minOccurs="0" maxOccurs="1" type="xsd:string" />
      <xsd:element name="editor" type="bb:DMLFieldEditor" minOccurs="0" maxOccurs="1" />
      <xsd:element name="parameterType" type="bb:DMLFieldDataType" minOccurs="0" maxOccurs="1" />
      <xsd:element name="validations" minOccurs="0" maxOccurs="1">
        <xsd:complexType>
          <xsd:choice minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="notNull" type="bb:DMLNotNullFieldValidator" />
            <xsd:element name="jsonSchema" type="bb:DMLJsonSchemaFieldValidator" />
            <xsd:element name="binary" type="bb:DMLBinaryFieldValidator" />
            <xsd:element name="string" type="bb:DMLStringFieldValidator" />
            <xsd:element name="regex" type="bb:DMLRegularExpressionFieldValidator" />
            <xsd:element name="custom" type="bb:DMLCustomValidator" />
            <xsd:element name="number" type="bb:DMLNumberFieldValidator" />
          </xsd:choice>
        </xsd:complexType>
      </xsd:element>
    </xsd:all>
    <xsd:attribute name="name" use="required" type="xsd:string" />
    <xsd:attribute name="displayName" use="optional" type="xsd:string" />
    <xsd:attribute name="allowNull" use="optional" type="xsd:boolean" />
    <xsd:attribute name="sourceType" type="bb:DMLCommandParameterSourceType" use="optional" />
    <xsd:attribute name="multiplicity" type="bb:DMLCommandParameterValueMultiplicity" use="optional" />
    <xsd:attribute name="order" use="optional" type="xsd:int" />
    <xsd:attribute name="ignoreGlobalFilter" use="optional" type="xsd:boolean" />
    <xsd:attribute name="isReadOnly" use="optional" type="xsd:boolean" />
    <xsd:attribute name="isVisible" use="optional" type="xsd:boolean" />
    <xsd:attribute name="logHistory" use="optional" type="xsd:boolean" />
  </xsd:complexType>
  <xsd:simpleType name="DMLCommandParameterSourceType">
    <xsd:annotation>
      <xsd:documentation>Source type of parameter</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="This">
        <xsd:annotation>
          <xsd:documentation>Locate parameter data from the property of selected entity.(this type is valid for model command and transition)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Self">
        <xsd:annotation>
          <xsd:documentation>The selected entity will be assigned to parameter.(this type is valid for model command and transition)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SystemDefined">
        <xsd:annotation>
          <xsd:documentation>System parameter like user id or cop</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="UserDefined">
        <xsd:annotation>
          <xsd:documentation>This type of parameter will be resolved from user entry</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="DMLCommandParameterValueMultiplicity">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Single" />
      <xsd:enumeration value="Multi" />
    </xsd:restriction>
  </xsd:simpleType>
