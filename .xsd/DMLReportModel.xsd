<?xml version="1.0" encoding="utf-8"?>
<xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.bigbang.io/dml/report" xmlns:bb="http://www.bigbang.io/dml/report">
  <xsd:element name="configuration">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="reportModel" type="bb:DMLReportModel" />
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="DMLReportModel">
    <xsd:all>
      <xsd:element name="description" minOccurs="0" maxOccurs="1" type="xsd:string" />
      <xsd:element name="icon" type="bb:DMLIcon" minOccurs="0" maxOccurs="1" />
      <xsd:element name="propertiesCollection" type="bb:DMLReportPropertyCollection" minOccurs="0" maxOccurs="1" />
      <xsd:element name="parametersCollection" type="bb:DMLParameterCollection" minOccurs="0" maxOccurs="1" />
      <xsd:element name="subReports" minOccurs="0" maxOccurs="1">
        <xsd:complexType>
          <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="subReport" type="bb:DMLSubReport" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="printTemplates" minOccurs="0" maxOccurs="1">
        <xsd:complexType>
          <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="printTemplate" type="bb:DMLPrintTemplate" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="bands" minOccurs="0" maxOccurs="1">
        <xsd:complexType>
          <xsd:choice minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="property" type="bb:DMLPropertyBand" />
            <xsd:element name="band" type="bb:DMLCustomBand" />
          </xsd:choice>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="navigations" minOccurs="0" maxOccurs="1">
        <xsd:complexType>
          <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="navigation" type="bb:DMLNavigation" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="toolBar" type="bb:DMLReportToolBar" minOccurs="0" maxOccurs="1" />
      <xsd:element name="databaseChannel" type="bb:DMLDatabaseChannel" minOccurs="0" maxOccurs="1" />
      <xsd:element name="externalReport" type="bb:DMLExternalReport" minOccurs="0" maxOccurs="1" />
    </xsd:all>
    <xsd:attribute name="relatedEntityModel" use="optional" type="xsd:string" />
    <xsd:attribute name="reportType" type="bb:DMLReportType" use="optional" />
    <xsd:attribute name="name" use="required" type="xsd:string" />
    <xsd:attribute name="dbName" use="optional" type="xsd:string" />
    <xsd:attribute name="displayName" use="optional" type="xsd:string" />
    <xsd:attribute name="moduleName" use="required" type="xsd:string" />
    <xsd:attribute name="parameterLayout" use="optional" type="xsd:string" />
    <xsd:attribute name="isActive" use="optional" type="xsd:boolean" />
    <xsd:attribute name="presentInDevelopment" use="optional" type="xsd:boolean" />
    <xsd:attribute name="defaultSearchProperty" use="optional" type="xsd:string" />
    <xsd:attribute name="partialLoad" use="optional" type="xsd:boolean" />
    <xsd:attribute name="aliasName" use="optional" type="xsd:string" />
    <xsd:attribute name="dbTimeoutPolicy" use="optional" type="xsd:string" />
  </xsd:complexType>
  <xsd:simpleType name="DMLReportType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Dynamic" />
      <xsd:enumeration value="Static" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="DMLIcon">
    <xsd:choice>
      <xsd:element name="simple" type="bb:DMLSimpleIcon" />
      <xsd:element name="font" type="bb:DMLFontIcon" />
      <xsd:element name="system" type="bb:DMLSystemIcon" />
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="DMLSimpleIcon">
    <xsd:all>
      <xsd:element name="content">
        <xsd:complexType>
          <xsd:attribute name="x-key" type="xsd:string" />
        </xsd:complexType>
      </xsd:element>
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="DMLFontIcon">
    <xsd:attribute name="name" use="required" type="xsd:string" />
    <xsd:attribute name="color" use="optional" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="DMLSystemIcon">
    <xsd:attribute name="type" type="bb:DMLSystemIconType" use="required" />
    <xsd:attribute name="color" use="optional" type="xsd:string" />
  </xsd:complexType>
  <xsd:simpleType name="DMLSystemIconType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Submit" />
      <xsd:enumeration value="Cancel" />
      <xsd:enumeration value="Apply" />
      <xsd:enumeration value="Settings" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="DMLReportPropertyCollection">
    <xsd:all>
      <xsd:element name="properties" minOccurs="0" maxOccurs="1">
        <xsd:complexType>
          <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="property" type="bb:DMLReportModelProperty" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="summaries" minOccurs="0" maxOccurs="1">
        <xsd:complexType>
          <xsd:choice minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="max" type="bb:DMLMaxSummary" />
            <xsd:element name="min" type="bb:DMLMinSummary" />
            <xsd:element name="sum" type="bb:DMLSumSummary" />
            <xsd:element name="average" type="bb:DMLAverageSummary" />
            <xsd:element name="custom" type="bb:DMLCustomSummary" />
          </xsd:choice>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="layoutExpressions" minOccurs="0" maxOccurs="1">
        <xsd:complexType>
          <xsd:choice minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="readOnlyField" type="bb:DMLFieldReadOnlyLayoutExpression" />
            <xsd:element name="disabledField" type="bb:DMLFieldDisabledLayoutExpression" />
            <xsd:element name="invisibleField" type="bb:DMLFieldInvisibleLayoutExpression" />
            <xsd:element name="readOnly" type="bb:DMLReadOnlyLayoutExpression" />
            <xsd:element name="disabled" type="bb:DMLDisabledLayoutExpression" />
            <xsd:element name="focusField" type="bb:DMLFieldFocusLayoutExpression" />
            <xsd:element name="backgroundColorField" type="bb:DMLFieldBackgroundColorLayoutExpression" />
            <xsd:element name="backgroundColor" type="bb:DMLBackgroundColorLayoutExpression" />
            <xsd:element name="foreColorField" type="bb:DMLFieldForeColorLayoutExpression" />
            <xsd:element name="foreColor" type="bb:DMLForeColorLayoutExpression" />
            <xsd:element name="captionColorField" type="bb:DMLFieldCaptionColorLayoutExpression" />
            <xsd:element name="textDecorationField" type="bb:DMLFieldTextDecorationLayoutExpression" />
            <xsd:element name="textDecoration" type="bb:DMLTextDecorationLayoutExpression" />
            <xsd:element name="fontStyleField" type="bb:DMLFieldFontStyleLayoutExpression" />
            <xsd:element name="fontStyle" type="bb:DMLFontStyleLayoutExpression" />
            <xsd:element name="filterField" type="bb:DMLFieldFilterLayoutExpression" />
            <xsd:element name="selectJsonTemplateField" type="bb:DMLFieldSelectJsonSchemaTemplateLayoutExpression" />
            <xsd:element name="filterEnumField" type="bb:DMLFieldEnumFilterLayoutExpression" />
            <xsd:element name="crossReferenceFilterField" type="bb:DMLFieldFilterCrossReferenceLayoutExpression" />
            <xsd:element name="customField" type="bb:DMLFieldCustomLayoutExpression" />
          </xsd:choice>
        </xsd:complexType>
      </xsd:element>
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="DMLReportModelProperty">
    <xsd:all>
      <xsd:element name="propertyType" type="bb:DMLFieldDataType" minOccurs="0" maxOccurs="1" />
      <xsd:element name="formatType" type="bb:DMLPropertyFormatType" minOccurs="0" maxOccurs="1" />
    </xsd:all>
    <xsd:attribute name="name" use="required" type="xsd:string" />
    <xsd:attribute name="displayName" use="optional" type="xsd:string" />
    <xsd:attribute name="order" use="optional" type="xsd:int" />
    <xsd:attribute name="allowNull" use="optional" type="xsd:boolean" />
    <xsd:attribute name="isVisible" use="optional" type="xsd:boolean" />
    <xsd:attribute name="isSortable" use="optional" type="xsd:boolean" />
    <xsd:attribute name="isSearchable" use="optional" type="xsd:boolean" />
    <xsd:attribute name="width" use="optional" type="xsd:string" />
    <xsd:attribute name="minWidth" use="optional" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="DMLFieldDataType">
    <xsd:choice>
      <xsd:element name="object" type="bb:DMLObjectFieldDataType" />
      <xsd:element name="dictionary" type="bb:DMLDictionaryFieldDataType" />
      <xsd:element name="model" type="bb:DMLModelFieldDataType" />
      <xsd:element name="crossReference" type="bb:DMLCrossReferenceFieldDataType" />
      <xsd:element name="list" type="bb:DMLListFieldDataType" />
      <xsd:element name="guid" type="bb:DMLGuidFieldDataType" />
      <xsd:element name="string" type="bb:DMLStringFieldDataType" />
      <xsd:element name="dateTime" type="bb:DMLDateTimeFieldDataType" />
      <xsd:element name="boolean" type="bb:DMLBooleanFieldDataType" />
      <xsd:element name="byte" type="bb:DMLByteFieldDataType" />
      <xsd:element name="short" type="bb:DMLShortFieldDataType" />
      <xsd:element name="int" type="bb:DMLIntFieldDataType" />
      <xsd:element name="long" type="bb:DMLLongFieldDataType" />
      <xsd:element name="float" type="bb:DMLFloatFieldDataType" />
      <xsd:element name="decimal" type="bb:DMLDecimalFieldDataType" />
      <xsd:element name="double" type="bb:DMLDoubleFieldDataType" />
      <xsd:element name="binary" type="bb:DMLBinaryFieldDataType" />
      <xsd:element name="file" type="bb:DMLFileFieldDataType" />
      <xsd:element name="image" type="bb:DMLImageFieldDataType" />
      <xsd:element name="version" type="bb:DMLVersionFieldDataType" />
      <xsd:element name="duration" type="bb:DMLDurationFieldDataType" />
      <xsd:element name="enum" type="bb:DMLEnumFieldDataType" />
      <xsd:element name="json" type="bb:DMLJsonFieldDataType" />
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="DMLObjectFieldDataType">
    <xsd:attribute name="specialType" use="required" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="DMLDictionaryFieldDataType">
    <xsd:all>
      <xsd:element name="valueType" type="bb:DMLDictionaryFilteredFieldDataType" />
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="DMLDictionaryFilteredFieldDataType">
    <xsd:choice>
      <xsd:element name="guid" type="bb:DMLGuidFieldDataType" />
      <xsd:element name="string" type="bb:DMLStringFieldDataType" />
      <xsd:element name="dateTime" type="bb:DMLDateTimeFieldDataType" />
      <xsd:element name="boolean" type="bb:DMLBooleanFieldDataType" />
      <xsd:element name="byte" type="bb:DMLByteFieldDataType" />
      <xsd:element name="short" type="bb:DMLShortFieldDataType" />
      <xsd:element name="int" type="bb:DMLIntFieldDataType" />
      <xsd:element name="long" type="bb:DMLLongFieldDataType" />
      <xsd:element name="float" type="bb:DMLFloatFieldDataType" />
      <xsd:element name="decimal" type="bb:DMLDecimalFieldDataType" />
      <xsd:element name="double" type="bb:DMLDoubleFieldDataType" />
      <xsd:element name="duration" type="bb:DMLDurationFieldDataType" />
      <xsd:element name="enum" type="bb:DMLEnumFieldDataType" />
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="DMLGuidFieldDataType" />
  <xsd:complexType name="DMLStringFieldDataType">
    <xsd:attribute name="stripHtmlTags" use="optional" type="xsd:boolean" />
  </xsd:complexType>
  <xsd:complexType name="DMLDateTimeFieldDataType" />
  <xsd:complexType name="DMLBooleanFieldDataType" />
  <xsd:complexType name="DMLByteFieldDataType" />
  <xsd:complexType name="DMLShortFieldDataType" />
  <xsd:complexType name="DMLIntFieldDataType" />
  <xsd:complexType name="DMLLongFieldDataType" />
  <xsd:complexType name="DMLFloatFieldDataType" />
  <xsd:complexType name="DMLDecimalFieldDataType" />
  <xsd:complexType name="DMLDoubleFieldDataType" />
  <xsd:complexType name="DMLDurationFieldDataType" />
  <xsd:complexType name="DMLEnumFieldDataType">
    <xsd:attribute name="name" use="required" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="DMLModelFieldDataType">
    <xsd:attribute name="sourceModel" use="required" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="DMLCrossReferenceFieldDataType">
    <xsd:all>
      <xsd:element name="entityModels" minOccurs="0" maxOccurs="1">
        <xsd:complexType>
          <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="entityModel" type="xsd:string" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:all>
    <xsd:attribute name="interface" use="optional" type="xsd:string" />
    <xsd:attribute name="useCrossReferenceTypeWhenInterfaceProvided" use="optional" type="xsd:boolean" />
  </xsd:complexType>
  <xsd:complexType name="DMLListFieldDataType">
    <xsd:all>
      <xsd:element name="elementType" type="bb:DMLListFilteredFieldDataType" />
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="DMLListFilteredFieldDataType">
    <xsd:choice>
      <xsd:element name="object" type="bb:DMLObjectFieldDataType" />
      <xsd:element name="dictionary" type="bb:DMLDictionaryFieldDataType" />
      <xsd:element name="model" type="bb:DMLModelFieldDataType" />
      <xsd:element name="crossReference" type="bb:DMLCrossReferenceFieldDataType" />
      <xsd:element name="guid" type="bb:DMLGuidFieldDataType" />
      <xsd:element name="string" type="bb:DMLStringFieldDataType" />
      <xsd:element name="dateTime" type="bb:DMLDateTimeFieldDataType" />
      <xsd:element name="boolean" type="bb:DMLBooleanFieldDataType" />
      <xsd:element name="byte" type="bb:DMLByteFieldDataType" />
      <xsd:element name="short" type="bb:DMLShortFieldDataType" />
      <xsd:element name="int" type="bb:DMLIntFieldDataType" />
      <xsd:element name="long" type="bb:DMLLongFieldDataType" />
      <xsd:element name="float" type="bb:DMLFloatFieldDataType" />
      <xsd:element name="decimal" type="bb:DMLDecimalFieldDataType" />
      <xsd:element name="double" type="bb:DMLDoubleFieldDataType" />
      <xsd:element name="binary" type="bb:DMLBinaryFieldDataType" />
      <xsd:element name="file" type="bb:DMLFileFieldDataType" />
      <xsd:element name="image" type="bb:DMLImageFieldDataType" />
      <xsd:element name="version" type="bb:DMLVersionFieldDataType" />
      <xsd:element name="duration" type="bb:DMLDurationFieldDataType" />
      <xsd:element name="enum" type="bb:DMLEnumFieldDataType" />
      <xsd:element name="json" type="bb:DMLJsonFieldDataType" />
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="DMLBinaryFieldDataType" />
  <xsd:complexType name="DMLFileFieldDataType" />
  <xsd:complexType name="DMLImageFieldDataType" />
  <xsd:complexType name="DMLVersionFieldDataType" />
  <xsd:complexType name="DMLJsonFieldDataType">
    <xsd:attribute name="schema" use="optional" type="xsd:string" />
    <xsd:attribute name="templateSelectionPolicy" type="bb:DMLJsonTemplateSelectionPolicy" use="optional" />
  </xsd:complexType>
  <xsd:simpleType name="DMLJsonTemplateSelectionPolicy">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Property" />
      <xsd:enumeration value="BeforeCreate" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="DMLPropertyFormatType">
    <xsd:choice>
      <xsd:element name="currency" type="bb:DMLCurrencyFormatType" />
      <xsd:element name="time" type="bb:DMLTimeFormatType" />
      <xsd:element name="date" type="bb:DMLDateFormatType" />
      <xsd:element name="dateTime" type="bb:DMLDateTimeFormatType" />
      <xsd:element name="url" type="bb:DMLUrlFormatType" />
      <xsd:element name="email" type="bb:DMLEmailFormatType" />
      <xsd:element name="duration" type="bb:DMLDurationFormatType" />
      <xsd:element name="digit" type="bb:DMLDigitFormatType" />
      <xsd:element name="custom" type="bb:DMLCustomFormatType" />
      <xsd:element name="stringFormat" type="bb:DMLStringFormatType" />
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="DMLCurrencyFormatType" />
  <xsd:complexType name="DMLTimeFormatType">
    <xsd:attribute name="durationType" type="bb:DMLTimeFormattingType" use="optional" />
  </xsd:complexType>
  <xsd:simpleType name="DMLTimeFormattingType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Hour" />
      <xsd:enumeration value="HourMinute" />
      <xsd:enumeration value="HourMinuteSecond" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="DMLDateFormatType" />
  <xsd:complexType name="DMLDateTimeFormatType">
    <xsd:attribute name="timeRemaining" use="optional" type="xsd:boolean" />
  </xsd:complexType>
  <xsd:complexType name="DMLUrlFormatType">
    <xsd:all>
      <xsd:element name="textExpression" minOccurs="0" maxOccurs="1" type="xsd:string" />
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="DMLEmailFormatType">
    <xsd:all>
      <xsd:element name="textExpression" minOccurs="0" maxOccurs="1" type="xsd:string" />
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="DMLDurationFormatType" />
  <xsd:complexType name="DMLDigitFormatType">
    <xsd:attribute name="scale" use="optional" type="xsd:int" />
  </xsd:complexType>
  <xsd:complexType name="DMLCustomFormatType">
    <xsd:all>
      <xsd:element name="formatting" minOccurs="1" maxOccurs="1" type="xsd:string" />
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="DMLStringFormatType">
    <xsd:attribute name="format" use="required" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="DMLMaxSummary">
    <xsd:all>
      <xsd:element name="formatType" type="bb:DMLPropertyFormatType" minOccurs="0" maxOccurs="1" />
    </xsd:all>
    <xsd:attribute name="property" use="required" type="xsd:string" />
    <xsd:attribute name="displayName" use="optional" type="xsd:string" />
    <xsd:attribute name="hideTitle" use="optional" type="xsd:boolean" />
  </xsd:complexType>
  <xsd:complexType name="DMLMinSummary">
    <xsd:all>
      <xsd:element name="formatType" type="bb:DMLPropertyFormatType" minOccurs="0" maxOccurs="1" />
    </xsd:all>
    <xsd:attribute name="property" use="required" type="xsd:string" />
    <xsd:attribute name="displayName" use="optional" type="xsd:string" />
    <xsd:attribute name="hideTitle" use="optional" type="xsd:boolean" />
  </xsd:complexType>
  <xsd:complexType name="DMLSumSummary">
    <xsd:all>
      <xsd:element name="formatType" type="bb:DMLPropertyFormatType" minOccurs="0" maxOccurs="1" />
    </xsd:all>
    <xsd:attribute name="property" use="required" type="xsd:string" />
    <xsd:attribute name="displayName" use="optional" type="xsd:string" />
    <xsd:attribute name="hideTitle" use="optional" type="xsd:boolean" />
  </xsd:complexType>
  <xsd:complexType name="DMLAverageSummary">
    <xsd:all>
      <xsd:element name="formatType" type="bb:DMLPropertyFormatType" minOccurs="0" maxOccurs="1" />
    </xsd:all>
    <xsd:attribute name="property" use="required" type="xsd:string" />
    <xsd:attribute name="displayName" use="optional" type="xsd:string" />
    <xsd:attribute name="hideTitle" use="optional" type="xsd:boolean" />
  </xsd:complexType>
  <xsd:complexType name="DMLCustomSummary">
    <xsd:all>
      <xsd:element name="aggregationExpression" minOccurs="0" maxOccurs="1" type="xsd:string" />
      <xsd:element name="formatType" type="bb:DMLPropertyFormatType" minOccurs="0" maxOccurs="1" />
    </xsd:all>
    <xsd:attribute name="provider" use="required" type="xsd:string" />
    <xsd:attribute name="property" use="optional" type="xsd:string" />
    <xsd:attribute name="name" use="required" type="xsd:string" />
    <xsd:attribute name="displayName" use="optional" type="xsd:string" />
    <xsd:attribute name="hideTitle" use="optional" type="xsd:boolean" />
  </xsd:complexType>
  <xsd:complexType name="DMLFieldReadOnlyLayoutExpression">
    <xsd:all>
      <xsd:element name="condition" minOccurs="0" maxOccurs="1" type="xsd:string" />
      <xsd:element name="multiBind" minOccurs="0" maxOccurs="1">
        <xsd:complexType>
          <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="multiBind" type="xsd:string" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:all>
    <xsd:attribute name="field" use="required" type="xsd:string" />
    <xsd:attribute name="name" use="optional" type="xsd:string" />
    <xsd:attribute name="displayName" use="optional" type="xsd:string" />
    <xsd:attribute name="mode" type="bb:DMLLayoutExpressionMode" use="optional" />
    <xsd:attribute name="bindTo" use="optional" type="xsd:string" />
  </xsd:complexType>
  <xsd:simpleType name="DMLLayoutExpressionMode">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="All" />
      <xsd:enumeration value="Present">
        <xsd:annotation>
          <xsd:documentation>Apply layout expression only on present (like grid and list)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="New" />
      <xsd:enumeration value="Edit" />
      <xsd:enumeration value="Form">
        <xsd:annotation>
          <xsd:documentation>Apply layout expression on edit and new</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="DMLFieldDisabledLayoutExpression">
    <xsd:all>
      <xsd:element name="condition" minOccurs="0" maxOccurs="1" type="xsd:string" />
      <xsd:element name="multiBind" minOccurs="0" maxOccurs="1">
        <xsd:complexType>
          <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="multiBind" type="xsd:string" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:all>
    <xsd:attribute name="field" use="required" type="xsd:string" />
    <xsd:attribute name="name" use="optional" type="xsd:string" />
    <xsd:attribute name="displayName" use="optional" type="xsd:string" />
    <xsd:attribute name="mode" type="bb:DMLLayoutExpressionMode" use="optional" />
    <xsd:attribute name="bindTo" use="optional" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="DMLFieldInvisibleLayoutExpression">
    <xsd:all>
      <xsd:element name="condition" minOccurs="0" maxOccurs="1" type="xsd:string" />
      <xsd:element name="multiBind" minOccurs="0" maxOccurs="1">
        <xsd:complexType>
          <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="multiBind" type="xsd:string" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:all>
    <xsd:attribute name="field" use="required" type="xsd:string" />
    <xsd:attribute name="name" use="optional" type="xsd:string" />
    <xsd:attribute name="displayName" use="optional" type="xsd:string" />
    <xsd:attribute name="mode" type="bb:DMLLayoutExpressionMode" use="optional" />
    <xsd:attribute name="bindTo" use="optional" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="DMLReadOnlyLayoutExpression">
    <xsd:all>
      <xsd:element name="condition" minOccurs="0" maxOccurs="1" type="xsd:string" />
    </xsd:all>
    <xsd:attribute name="name" use="optional" type="xsd:string" />
    <xsd:attribute name="displayName" use="optional" type="xsd:string" />
    <xsd:attribute name="mode" type="bb:DMLLayoutExpressionMode" use="optional" />
  </xsd:complexType>
